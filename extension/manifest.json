{
  "manifest_version": 3,
  "name": "HAIETMOBA",
  "version": "1.0",
  "description": "A moment-by-moment journal of your experience with automatic sync across all your Chrome browsers.",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_title": "Open HAIETMOBA Journal"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}